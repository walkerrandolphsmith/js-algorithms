{
  "name": "@quillio/modTen",
  "version": "1.0.0",
  "description": "Luhn's algorithm",
  "main": "index.js",
  "scripts": {
    "test": "ava",
    "build": "babel src --out-dir dist --ignore test.js && uglifyjs ./dist/index.js -o ./dist/index.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Quillio/modTen.git"
  },
  "keywords": [
    "Luhn algorithm",
    "Luhn formula",
    "modTen"
  ],
  "author": "admin@quillio.io",
  "contributors": ["walkerrandolphsmith@gmail.com"],
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/Quillio/modTen/issues"
  },
  "homepage": "https://github.com/Quillio/modTen#readme",
  "devDependencies": {
    "ava": "^0.22.0",
    "babel-cli": "^6.26.0",
    "babel-preset-es2015": "^6.24.1",
    "uglify-js": "^3.1.0"
  },
  "babel": {
    "presets": [
      "es2015"
    ]
  },
  "ava": {
    "files": [
      "src/**/*.test.js"
    ],
    "source": [
      "src/**/*.js"
    ],
    "match": [],
    "concurrency": 5,
    "failFast": true,
    "failWithoutAssertions": false,
    "tap": true,
    "powerAssert": false,
    "require": [
      "babel-register"
    ],
    "babel": "inherit"
  }
}
